    <script>
        function copyIP() {
            var $temp = $("<input>");
            $("body").append($temp);
            $temp.val($('#ip').html()).select();
            document.execCommand("copy");
            $temp.remove();

            Swal.fire({
                icon: 'success',
                title: '<span style="color:#fff; font-size: 25px">Đã copy IP!</span>',
                html: '<span style="color:white; font-size: 14px">Chúc bạn vui vẻ tại Thicc SMP!</span>',
                background: 'rgb(41,41,41)',
                timer: 3000,
                showConfirmButton: false
            })
        }

        if (window.innerWidth > 767) {
            $('#home-nav').css("border-bottom-color", "#fff");
            $('#home-nav').css("border-bottom-style", "solid");
        }

        window.onresize = function(e) {
            if (window.innerWidth > 767) {
                $('#home-nav').css("border-bottom-color", "#fff");
                $('#home-nav').css("border-bottom-style", "solid");
            } else {
                $('#home-nav').css("border-bottom-style", "none");
            }
        }

        window.onscroll = function(e) {
            if (window.scrollY < 950 && window.innerWidth > 767) {
                $('#home-nav').css("border-bottom-color", "#fff");
                $('#home-nav').css("border-bottom-style", "solid");
            } else {
                $('#home-nav').css("border-bottom-style", "none");
            }

            if (window.scrollY >= 950 && window.scrollY < 3500 && window.innerWidth > 767) {
                $('#info-nav').css("border-bottom-color", "#fff");
                $('#info-nav').css("border-bottom-style", "solid");
            } else {
                $('#info-nav').css("border-bottom-style", "none");
            }

            if (window.scrollY >= 3500 && window.scrollY < 4000 && window.innerWidth > 767) {
                $('#faq-nav').css("border-bottom-color", "#fff");
                $('#faq-nav').css("border-bottom-style", "solid");
            } else {
                $('#faq-nav').css("border-bottom-style", "none");
            }

            if (window.scrollY >= 4000 && window.innerWidth > 767) {
                $('#support-nav').css("border-bottom-color", "#fff");
                $('#support-nav').css("border-bottom-style", "solid");
            } else {
                $('#support-nav').css("border-bottom-style", "none");
            }
        };

        setInterval(() => {
            var date = new Date();
            $("#time").text((date.getHours() < 10 ? '0' : '') + date.getHours() + ":" + (date.getMinutes() < 10 ? '0' : '') + date.getMinutes() + ":" + (date.getSeconds() < 10 ? '0' : '') + date.getSeconds());
            if (date.getHours() < 18 && date.getHours() >= 6) {
                $("#time-icon").removeClass("fa-moon");
                $("#time-icon").addClass("fa-sun");
            } else {
                $("#time-icon").removeClass("fa-sun");
                $("#time-icon").addClass("fa-moon");
            }

            var timezone = date.getTimezoneOffset();
            timezone = (timezone / 60) * -1;
            var gmt = 'GMT';
            if (timezone !== 0) {
                gmt += timezone > 0 ? '+' : '';
                gmt += timezone;
            }
            $("#timezone").text(gmt);
        }, 1000);
    </script>
